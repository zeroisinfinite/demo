  .slide {
    /* Required Styles */
    position:relative;
    width:550px;
    height:310px;
    overflow:hidden;
    margin: 0 auto;
    border:10px solid #444;
    border-radius:5px;
    box-shadow:0 0 6px rgba(0, 0, 0, .4);
  }
  .slide img {
    display:block;
    width:550px;
    height:310px;
  }
  .slide img {
    position:absolute;
  }
  .switchable-triggers {
    width:570px;
    height:30px;
    overflow-y:hidden;
    line-height:30px;
    text-align:center;
    position: absolute;
    right: 3px;
    bottom: 6px;
  }
  .switchable-triggers li {
    display: inline-block;
    float: left;
    width: 20px;
    height: 20px;
    margin: 0 3px;
    border-radius: 20px;
    background: rgba(255, 255, 255, .7);
    color: #444;
    font-size: 13px;
    line-height: 20px;
    text-align: center;
    cursor: pointer;
    *display: inline;
    *zoom: 1;
  }
  .switchable-triggers li.current {
    background: rgba(138, 181, 22, .7);
    color: #fff;
  }
  .button {
      width: 100%;
      height: 32px;
      margin-top: 10px;
      text-align: center;
  }
@media (max-width : 767px) {
.switchable-triggers {width: 340px;}
.slide,.slide img{width: 330px;height: 186px;}
.productPictures img{width: 330px;}
}
</style>
