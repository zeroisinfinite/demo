<script type="text/javascript">
    jQuery(document).ready(function(){
        var $ = jQuery;

        function init(){
            var params = YouHooUtils.parseAnchor();
            console.log(params);
            params && params.tid && doSearchData(params.tid)
        }
        function doSearchData(productId) {
            //异步请求，需加loading中的显示
            $.ajax({
                url: myConfig.searchProduct
                ,type: "get"
                ,dataType: "json"
            })
            .done(function (data) {
                var _html = "";
                // baseUrl ../images/upload/product/101/switch/
                console.log("==searchProduct== ",data)
                for (var i = 0; i < data.length; i++) {
                  var _obj = data[i];
                  if (_obj.id == productId) {
                    renderView(_obj);
                  };
                };        
                
            })
            .error(function (d) {
            })
            .always(function () {
            });
        }    
        /**
         *  <img src="../images/demo/p1.jpg" style="opacity: 1; z-index: 7;">
         * "price": 1356.00,
         * "keyWords":["防干涩、防缺氧、抗疲劳","防辐射功能","真空密封保存,全程除菌"],
         * "detailUrls"  : ["20150722115648_5468.jpg","20150722115648_5468.jpg"],
         * "switchUrls" : ["p1.jpg","p2.jpg","p3.jpg","p4.jpg","p5.jpg"]
         */   
        function renderView(obj) {
          var _basePath = '../images/upload/product/101/';
          // var _basePath2 = '../imgFile/version2/product/';
          obj.price && $('#infoProductBlockTop').find('.priceValue').html(obj.price);
          obj.title && $('#productName').find('h1').html(obj.title);
          if (obj.keyWords && obj.keyWords.length) {
            var _html = "";
            for (var i = 0; i < obj.keyWords.length; i++) {
              _html += '<li>- '+ obj.keyWords[i] +'</li>';
            };
            $('#productDescription').find('.functional-text').html(_html+ "<br>");  
          };
          if (obj.switchUrls && obj.switchUrls.length) {
            var _html = "";
            for (var i = 0; i < obj.switchUrls.length; i++) {
              _html += '<img src="'+ _basePath + "switch/" + obj.switchUrls[i] +'" style="opacity: '+(i?1:1)+'; z-index: '+(i?7:1)+';">';
            };
            $('#slide-box .J-img-container').html(_html); 
            
            setTimeout(function(e){
              // the following should be after picView render
              doSwitchPicView();
            },500)
            
          }
          //<li> <img src="../images/upload/product/101/detail/20150722115648_5468.jpg"> </li>
          if (obj.detailUrls && obj.detailUrls.length) {
            var _html = "";
            for (var i = 0; i < obj.detailUrls.length; i++) {
              _html += '<li> <img src="'+ _basePath + "list/" + obj.detailUrls[i] +'" </li>';
            };
            $('#productPictures').html(_html);  
          };
        }
        function doSwitchPicView() {
          var Slide = $('#slide-box').switchable({
            putTriggers: 'appendTo',
            panels: 'li',
            initIndex: -1, // display the last panel
            effect: 'scrollRight', // taking effect when autoplay == true
            easing: 'cubic-bezier(.455, .03, .515, .955)', // equal to 'easeInOutQuad'
            end2end: true, // if set to true, loop == true
            loop: false, // not taking effect, because end2end == true
            autoplay: true,
            interval: 5,
            api: true // if set to true, Switchable returns API
          }).pause();

          var btn = $('#j-button');
          btn.click(function(e){
            e.preventDefault();

            if ( Slide.paused ) {
              Slide.play();
              btn.text('Pause');
            }
            else {
              Slide.pause();
              btn.text('Play');
            }
          });
          // $('#slide-box').switchable({
          //   triggers: '&bull;',
          //   effect: 'fade'
          // });
        }
        init();
    })
</script>